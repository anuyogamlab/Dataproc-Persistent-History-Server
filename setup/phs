gcloud dataproc clusters create persistent-history-server --region us-central1 --zone us-central1-b --enable-component-gateway --single-node --master-machine-type n2-highmem-8 --master-boot-disk-size 500 --image-version 2.0-debian10 --properties yarn:yarn.nodemanager.remote-app-log-dir=gs://persistent-history-server/yarn-logs,mapred:mapreduce.jobhistory.done-dir=gs://persistent-history-server/events/mapreduce-job-history/done,mapred:mapreduce.jobhistory.intermediate-done-dir=gs://persistent-history-server/events/mapreduce-job-history/intermediate-done,spark:spark.eventLog.dir=gs://persistent-history-server/events/spark-job-history,spark:spark.history.fs.logDirectory=gs://persistent-history-server/events/spark-job-history,spark:spark.history.fs.gs.outputstream.type=FLUSHABLE_COMPOSITE,spark:spark.history.fs.gs.outputstream.sync.min.interval.ms=1000ms --project <PROJECT NAME>
